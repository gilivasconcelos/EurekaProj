<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AutenticarServlet</servlet-name>
        <servlet-class>.AutenticarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>autenticaUsuario</servlet-name>
        <servlet-class>autenticaUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Navegacao</servlet-name>
        <servlet-class>Navegacao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dataGrid</servlet-name>
        <servlet-class>dataGrid</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AutenticarServlet</servlet-name>
        <url-pattern>/AutenticarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>autenticaUsuario</servlet-name>
        <url-pattern>/autenticaUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Navegacao</servlet-name>
        <url-pattern>/Navegacao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dataGrid</servlet-name>
        <url-pattern>/dataGrid</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
